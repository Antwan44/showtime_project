 fixed-tabs>\n          <v-tabs-slider></v-tabs-slider>\n          <v-tab href=\"#mobile-tabs-5-1\" class=\"teal--text\">\n            <v-icon>mdi-account-box</v-icon>\n          </v-tab>\n\n          <v-tab href=\"#mobile-tabs-5-2\" class=\"teal--text\">\n            <v-icon>mdi-music</v-icon>\n          </v-tab>\n\n          <v-tab href=\"#mobile-tabs-5-3\" class=\"teal--text\">\n            <v-icon>mdi-cart</v-icon>\n          </v-tab>\n        </v-tabs>\n      </template>\n    </v-toolbar>\n\n    <v-tabs-items v-model=\"tabs\">\n      <v-tab-item value=\"mobile-tabs-5-1\">\n        <v-card flat>\n          <base-v-component heading=\"Simple Tables\" link=\"components/simple-tables\" />\n\n          <base-material-card icon=\"mdi-clipboard-text\" title=\"Simple Table\" class=\"px-5 py-3\">\n            <v-simple-table>\n              <thead>\n                <tr>\n                  <th class=\"teal--text\">\n                    ID\n                  </th>\n                  <th class=\"teal--text\">\n                    Name\n                  </th>\n                  <th class=\"teal--text\">\n                    Email\n                  </th>\n                  <th class=\"teal--text\">\n                    Phone\n                  </th>\n                     <th class=\"teal--text\">\n                    Admin\n                  </th>\n                  <th class=\" teal--text\">\n                    Edit\n                  </th>\n                  <th class=\" teal--text\">\n                    Delete\n                  </th>\n                </tr>\n              </thead>\n\n              <tbody>\n                <tr v-for=\"users in getAllUser\" :key=\"users.id\">\n                  <td>{{users._id}}</td>\n                  <td>{{users.firstname}}</td>\n                  <td>{{users.email}}</td>\n                  <td>{{users.phone}}</td>\n                  <td>{{users.admin}}</td>\n                  <td>\n\n                    <div @click=\"id = users._id\" data-toggle=\"modal\" :data-target=\"'#exampleModal'+users._id\"\n                      class=\"bleu\"><i class=\"far fa-edit\"></i></div>\n\n\n                    <div class=\"modal fade\" :id=\"'exampleModal'+users._id\" tabindex=\"-1\" role=\"dialog\"\n                      aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n                      <div class=\"modal-dialog\" role=\"document\">\n                        <div class=\"modal-content\">\n                          <v-card data-app>\n                            <form @submit.prevent=\"onEdit(users)\" class=\"sign-back\">\n\n                              <v-card-title>\n                                <span class=\"headline\">User Profile</span>\n                              </v-card-title>\n                              <v-card-text>\n                                <v-container>\n                                  <v-row>\n                                    <v-col cols=\"12\" sm=\"8\" md=\"5\">\n                                      <v-text-field label=\"Firstname\" hint=\"Required\" required\n                                        v-model=\"users.firstname\">\n                                      </v-text-field>\n                                    </v-col>\n                                    <v-col cols=\"12\" sm=\"8\" md=\"5\">\n                                      <v-text-field label=\"Lastname\" hint=\"Required\" v-model=\"users.lastname\">\n                                      </v-text-field>\n                                    </v-col>\n                                    <v-col cols=\"12\" sm=\"8\" md=\"5\">\n                                      <v-text-field label=\"Email*\" required v-model=\"users.email\"></v-text-field>\n                                    </v-col>\n                                    <v-col cols=\"12\" sm=\"5\" md=\"5\">\n                                      <v-text-field label=\"Phone Number\" hint=\"Required\" v-model=\"users.phone\">\n                                      </v-text-field>\n                                    </v-col>\n                                    <label for=\"admin\" >Admin Rights</label>\n\n                                    <select name=\"admin\" id=\"admin\" class=\"custom-select\" v-model=\"users.admin\">\n                                 \n                                      <option value=\"true\">Admin</option>\n                                      <option value=\"false\">Visitor</option>\n\n                                    </select>\n\n                                  </v-row>\n\n                                </v-container>\n                              </v-card-text>\n                              <v-card-actions>\n                                <v-spacer></v-spacer>\n                                <v-btn color=\"blue darken-1\" text @click=\"dialog = false\" data-dismiss=\"modal\">Close\n                                </v-btn>\n                                <v-btn color=\"blue darken-1\" type=\"submit\" text @click=\"dialog = false\">Save</v-btn>\n                              </v-card-actions>\n                            </form>\n                          </v-card>\n                        </div>\n                      </div>\n                    </div>\n\n\n\n\n                  </td>\n                  <td> <a href=\"#\" class=\"icon\"><i v-on:click.prevent=\"onDelete(users._id)\"\n                        class=\"fas fa-trash\"></i></a></td>\n                </tr>\n\n\n              </tbody>\n            </v-simple-table>\n          </base-material-card>\n        </v-card>\n      </v-tab-item>\n      <v-tab-item value=\"mobile-tabs-5-2\">\n        <v-card flat>\n          <base-v-component heading=\"Simple Tables\" link=\"components/simple-tables\" />\n\n          <base-material-card icon=\"mdi-clipboard-text\" title=\"Simple Table\" class=\"px-5 py-3\">\n            <v-simple-table>\n              <thead>\n                <tr>\n                  <th class=\"teal--text\">\n                    ID\n                  </th>\n                  <th class=\"teal--text\">\n                    Title\n                  </th>\n                  <th class=\"teal--text\">\n                    Location\n                  </th>\n                  <th class=\"teal--text\">\n                    Band\n                  </th>\n                  <th class=\" teal--text\">\n                    Edit\n                  </th>\n                  <th class=\" teal--text\">\n                    Delete\n                  </th>\n                </tr>\n              </thead>\n\n              <tbody>\n                <tr v-for=\"concert in getAllConcert\" :key=\"concert.id\">\n                  <td>{{concert._id}}</td>\n                  <td>{{concert.title}}</td>\n                  <td>{{concert.location}}</td>\n                  <td>{{concert.band}}</td>\n\n                  <td>\n                    <i class=\"far fa-edit\"></i>\n                  </td>\n                  <td> <i v-on:click.prevent=\"onDeleteConcert(concert._id)\" class=\"fas fa-trash\"></i></td>\n\n                </tr>\n\n\n              </tbody>\n            </v-simple-table>\n          </base-material-card>\n        </v-card>\n      </v-tab-item>\n      <v-tab-item value=\"mobile-tabs-5-3\">\n        <v-card flat>\n          <base-v-component heading=\"Simple Tables\" link=\"components/simple-tables\" />\n\n          <base-material-card icon=\"mdi-clipboard-text\" title=\"Simple Table\" class=\"px-5 py-3\">\n            <v-simple-table>\n              <thead>\n                <tr>\n                  <th class=\"teal--text\">\n                    ID\n                  </th>\n                  <th class=\"teal--text\">\n                    User\n                  </th>\n                  <th class=\"teal--text\">\n                    Concert\n                  </th>\n                  <!--    <th class=\"teal--text\">\n        Price\n      </th> -->\n                  <th class=\" teal--text\">\n                    Edit\n                  </th>\n                  <th class=\" teal--text\">\n                    Delete\n                  </th>\n                </tr>\n              </thead>\n\n              <tbody>\n                <tr v-for=\"tickets in getAllTicket\" :key=\"tickets.id\">\n                  <td>{{tickets._id}}</td>\n                  <td>{{tickets.userId}}</td>\n                  <td>{{tickets.concertId}}</td>\n                  <!--       <td>Oud-Turnhout</td> -->\n                  <td>\n                    <i class=\"far fa-edit\"></i>\n                  </td>\n                  <td> <i v-on:click.prevent=\"onDeleteTicket(tickets._id)\" class=\"fas fa-trash\"></i></td>\n\n                </tr>\n\n\n              </tbody>\n            </v-simple-table>\n          </base-material-card>\n        </v-card>\n      </v-tab-item>\n    </v-tabs-items>\n\n  </v-card>\n</template>\n\n<script>\n  import {\n    mapActions,\n    mapGetters\n  } from 'vuex'\n\n  export default {\n    data() {\n      return {\n        tabs: null,\n        dialog: false,\n        id: '',\n        lastname: '',\n        firstname: '',\n        email: '',\n        admin: '',\n        password: '',\n        confirmation_password: '',\n        /*      editUser: {\n               'id': '',\n               'lastname': '',\n               'firstname': '',\n               'email': '',\n               'phone': '',\n               'password': '',\n               'confirmation_password': '',\n               'admin': false,\n             } */\n\n      }\n    },\n    methods: {\n      ...mapActions(['fetchAllUser', 'fetchUser', 'fetchAllConcert', 'fetchAllTicket', 'createUser', 'editUserForm',\n        'Deleteticket',\n        'fetchUser', 'deleteUser'\n      ]),\n\n      onsubmit() {\n\n        var obj = {\n          '_id': this._id,\n          'firstname': this.firstname,\n          'lastname': this.lastname,\n          'email': this.email,\n          'phone': this.phone,\n          'password': this.password,\n          'confirmation_password': this.confirmation_password,\n          'admin': this.admin,\n        }\n        this.createUser(obj);\n        this.fetchAllUser();\n\n      },\n\n      onEdit(user) {\n\n/*         if (user.admin == 'Visitor') {\n          user.admin = false;\n        } else if (user.admin == 'Admin') {\n          user.admin = true;\n        } */\n\n\n        var obj = {\n          'id': this.id,\n          'firstname': user.firstname,\n          'lastname': user.lastname,\n          'email': user.email,\n          'admin': user.admin,\n          'phone': user.phone,\n\n        }\n        this.editUserForm(obj);\n        this.fetchAllUser();\n\n      },\n\n      onDelete(id) {\n        this.fetchAllUser();\n        this.deleteUser(id)\n        this.fetchAllUser();\n\n      },\n      onDeleteConcert(id) {\n        this.fetchAllConcert();\n        this.deleteConcert(id)\n        this.fetchAllConcert();\n      },\n      onDeleteTicket(id) {\n        this.fetchAllTicket();\n        this.Deleteticket(id)\n        this.fetchAllTicket();\n      }\n\n\n\n\n    },\n    computed: {\n      ...mapGetters(['getAllUser', 'getAllTicket', 'getAllConcert', 'getMessage', 'getUser', 'infoUserById']),\n\n    },\n    created() {\n      this.fetchAllUser();\n      this.fetchAllConcert();\n      this.fetchAllTicket();\n\n    }\n  }\n</script>\n<style scoped>\n\n\n\n</style>"]}]}